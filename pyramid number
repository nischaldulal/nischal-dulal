#include<stdio.h>
int main(){
     int n ,num=1;
     printf("enter the number of rows");
    scanf("%d",&n);
    for (int i=1;i<=n; i++){
        for( int k=1;k<=n-i; k++)
        printf(" ");
    for (int j=1;j<=num;j++){
        printf("%d",num);
      } 
       num=num+2;
printf("\n");
    }
    return 0;
}